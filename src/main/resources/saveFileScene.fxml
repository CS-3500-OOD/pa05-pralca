<Scene fx:id="saveFileScene" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200" prefWidth="200">
      <children>
         <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="200.0" stroke="BLACK" strokeType="INSIDE" width="200.0" />
         <Canvas height="200.0" width="200.0" />
         <Text fx:id="saveTitle" layoutX="79.0" layoutY="19.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Save">
            <font>
               <Font name="Futura Medium" size="16.0" />
            </font>
         </Text>
         <Text fx:id="nameOfFile" layoutX="6.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Absolute Path:">
            <font>
               <Font name="Futura Medium" size="13.0" />
            </font>
         </Text>
         <TextField fx:id="savePath" layoutX="92.0" layoutY="69.0" prefHeight="26.0" prefWidth="102.0" />
      </children></AnchorPane>
</Scene>
